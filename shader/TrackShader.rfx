     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[



float4 materialAmbientColor;
float4 materialSpecularColor;
float  materialGlossiness;

float4 lightAmbientColor;

float4 lightDiffuseColor0;
float4 lightDiffuseColor1;
float4 lightDiffuseColor2;
float4 lightDiffuseColor3;

float4 lightSpecularColor0;
float4 lightSpecularColor1;
float4 lightSpecularColor2;
float4 lightSpecularColor3;

sampler2D DiffuseTexture;
sampler2D NormalMap;

int paralaxSteps=10;



inline float3 reflectionVector(float3 v,float3 reflectAbout)
{
   float3 ret;
   ret=(2.0f*(dot(reflectAbout,v)))*reflectAbout-v;
   return ret;
}



//compute ligthing with the phong model for specular lighting
float4 diffuse_specular_phong(float3 lightdir, float3 normal, float3 viewdir,
                             float4 diffuseColor,
                             float lightcolorSpecular,float lightcolorDiffuse)
{
   float3 reflected=reflectionVector(lightdir,normal);
   //specular lighting
   float4 ret=(pow(max(dot(viewdir,reflected),0),materialGlossiness)*lightcolorSpecular) * materialSpecularColor;
   //diffuse lighting
   ret+=(max(dot(normal,lightdir),0)*lightcolorDiffuse)*diffuseColor;
   return ret;
}

//compute ligthing with the blinn_phong model for specular lighting
float4 diffuse_specular_blinn(float3 lightdir, float3 normal, float3 viewdir,
                             float4 diffuseColor,
                             float lightcolorSpecular,float lightcolorDiffuse)
{
   
   float3 h,vl;
   vl=viewdir+lightdir;
   h=vl/length(vl);
   //specular lighting
   float ret=(pow(max(dot(normal,h),0),materialGlossiness)*lightcolorSpecular) * materialSpecularColor;
   //diffuse lighting
   ret+=(max(dot(normal,lightdir),0)*lightcolorDiffuse)*diffuseColor; 
   return ret;
}

float2 paralaxMapping(float3 viewdir,inout float2 texcoord)
{
   
   float mapHeight= tex2D(NormalMap,texcoord).a;
   float height=1.0f;
   float paralaxStepSize=1.0f/paralaxSteps;
   float2 stepDim=float2(-viewdir.x,viewdir.y)/(viewdir.z*paralaxSteps*30);
   
   for(int i=0;i<paralaxSteps;i++)
   {
      if(mapHeight<height)
      {
         height-=paralaxStepSize;
         texcoord+=stepDim;
         mapHeight= tex2D(NormalMap,texcoord).w;
      }
   }
   height+=paralaxStepSize;
   texcoord-=stepDim;
   paralaxStepSize/=paralaxSteps;
   stepDim/=paralaxSteps;
   mapHeight= tex2D(NormalMap,texcoord).w;
   for(int i=0;i<paralaxSteps;i++)
   {
      if(mapHeight<height)
      {
         height-=paralaxStepSize;
         texcoord+=stepDim;
         mapHeight= tex2D(NormalMap,texcoord).w;
      }
   }
   
   return texcoord;
}


float4 ps_main(float4 ScreenPos          :POSITION,
               float2 texcoord           :TEXCOORD0,
               float3 ViewSpaceViewDir   :TEXCOORD1, 
               float4 ViewSpaceLight0Dir :TEXCOORD2,
               float4 ViewSpaceLight1Dir :TEXCOORD3,
               float4 ViewSpaceLight2Dir :TEXCOORD4,
               float4 ViewSpaceLight3Dir :TEXCOORD5) : COLOR0
{   
   ViewSpaceViewDir = normalize(ViewSpaceViewDir);
  texcoord=paralaxMapping(ViewSpaceViewDir,texcoord);
   float3 ViewSpaceNormal=2.0f*(tex2D(NormalMap,texcoord)-0.5f);//normalize(ViewSpaceNormal);
   
    float texelColor=tex2D(DiffuseTexture,texcoord);
   
   float fragmentColor=lightAmbientColor*texelColor;
   
      
   ViewSpaceLight0Dir.xyz = normalize(ViewSpaceLight0Dir.xyz);
   ViewSpaceLight1Dir.xyz = normalize(ViewSpaceLight1Dir.xyz);
   ViewSpaceLight2Dir.xyz = normalize(ViewSpaceLight2Dir.xyz);
   ViewSpaceLight3Dir.xyz = normalize(ViewSpaceLight3Dir.xyz);
   fragmentColor+=diffuse_specular_blinn(ViewSpaceLight0Dir.xyz,ViewSpaceNormal,ViewSpaceViewDir,texelColor,lightSpecularColor0,lightDiffuseColor0);
   fragmentColor+=diffuse_specular_blinn(ViewSpaceLight1Dir.xyz,ViewSpaceNormal,ViewSpaceViewDir,texelColor,lightSpecularColor1,lightDiffuseColor1);
   fragmentColor+=diffuse_specular_blinn(ViewSpaceLight2Dir.xyz,ViewSpaceNormal,ViewSpaceViewDir,texelColor,lightSpecularColor2,lightDiffuseColor2);
   fragmentColor+=diffuse_specular_blinn(ViewSpaceLight3Dir.xyz,ViewSpaceNormal,ViewSpaceViewDir,texelColor,lightSpecularColor3,lightDiffuseColor3);
   
   return fragmentColor;
   
}




]]></RmShaderCode>
