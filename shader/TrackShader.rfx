float4x4 view;
float4x4 worldView;
float4x4 worldViewIT;
float4x4 worldViewProjection;
float4x4 lightWorldViewProjection0;
float4x4 lightWorldViewProjection1;
float4x4 lightWorldViewProjection2;
float4x4 lightWorldViewProjection3;

float maxDist;

float3 lightPosition0;
float3 lightPosition1;
float3 lightPosition2;
float3 lightPosition3;

float maxLightRangeSQ;

struct VS_INPUT
{
   float4 Position : POSITION;
   float3 Normal   : NORMAL;
   float2 TexCoord : TEXCOORD0;
   float3 Tangent  : TANGENT;
};

struct VS_OUTPUT 
{
   float4 ScreenPos          : POSITION;
   float3 ViewSpacePos       : TEXCOORD0;
   float2 TexCoord           : TEXCOORD1;
   float4 ViewSpaceLightDir0 : TEXCOORD2;
   float4 ViewSpaceLightDir1 : TEXCOORD3;
   float4 ViewSpaceLightDir2 : TEXCOORD4;
   float4 ViewSpaceLightDir3 : TEXCOORD5;
   float4 ShadowMapTexCoord0 : TEXCOORD6;
   float4 ShadowMapTexCoord1 : TEXCOORD7;
   float4 ShadowMapTexCoord2 : TEXCOORD8;
   float4 ShadowMapTexCoord3 : TEXCOORD9;
};


VS_OUTPUT vs_main( VS_INPUT In )
{
   VS_OUTPUT Out;
   
   float4 viewPos   = -mul(In.Position, worldView);
   
   Out.ScreenPos    = mul(In.Position, worldViewProjection);
   Out.TexCoord     = In.TexCoord;
   Out.ViewSpacePos = mul(In.Position, worldView);
   
   Out.ShadowMapTexCoord0   = mul(In.Position, lightWorldViewProjection0);
   Out.ViewSpaceLightDir0   = mul(float4(lightPosition0,1),view) - viewPos;
   Out.ViewSpaceLightDir0.w = Out.ShadowMapTexCoord0.z / maxDist;
   
   Out.ShadowMapTexCoord1   = mul(In.Position, lightWorldViewProjection1);
   Out.ViewSpaceLightDir1   = mul(float4(lightPosition1,1),view) - viewPos;
   Out.ViewSpaceLightDir1.w = Out.ShadowMapTexCoord1.z / maxDist;
   
   Out.ShadowMapTexCoord2   = mul(In.Position, lightWorldViewProjection2);
   Out.ViewSpaceLightDir2   = mul(float4(lightPosition2,1),view) - viewPos;
   Out.ViewSpaceLightDir2.w = Out.ShadowMapTexCoord2.z / maxDist;
   
   Out.ShadowMapTexCoord3   = mul(In.Position, lightWorldViewProjection3);
   Out.ViewSpaceLightDir3   = mul(float4(lightPosition3,1),view) - viewPos;
   Out.ViewSpaceLightDir3.w = Out.ShadowMapTexCoord3.z / maxDist;
   
   return Out;  
}



