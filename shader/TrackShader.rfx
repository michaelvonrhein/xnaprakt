float4 materialAmbientColor;
float4 materialSpecularColor;
float  materialGlossiness;

float4 lightAmbientColor;

float4 lightDiffuseColor0;
float4 lightDiffuseColor1;
float4 lightDiffuseColor2;
float4 lightDiffuseColor3;

float4 lightSpecularColor0;
float4 lightSpecularColor1;
float4 lightSpecularColor2;
float4 lightSpecularColor3;

sampler2D DiffuseTexture;


inline float3 reflectionVector(float3 v,float3 reflectAbout)
{
   float3 ret;
   ret=(2.0f*(dot(reflectAbout,v)))*reflectAbout-v;
   return ret;
}



//compute ligthing with the phong model for specular lighting
float4 diffuse_specular_phong(float3 lightdir, float3 normal, float3 viewdir,
                             float4 diffuseColor,
                             float lightcolorSpecular,float lightcolorDiffuse)
{
   float3 reflected=reflectionVector(lightdir,normal);
   //specular lighting
   float4 ret=(pow(max(dot(viewdir,reflected),0),materialGlossiness)*lightcolorSpecular) * materialSpecularColor;
   //diffuse lighting
   ret+=(max(dot(normal,lightdir),0)*lightcolorDiffuse)*diffuseColor;
   return ret;
}

//compute ligthing with the blinn_phong model for specular lighting
float4 diffuse_specular_blinn(float3 lightdir, float3 normal, float3 viewdir,
                             float4 diffuseColor,
                             float lightcolorSpecular,float lightcolorDiffuse)
{
   
   float3 h,vl;
   vl=viewdir+lightdir;
   h=vl/length(vl);
   //specular lighting
   float ret=(pow(max(dot(normal,h),0),materialGlossiness)*lightcolorSpecular) * materialSpecularColor;
   //diffuse lighting
   ret+=(max(dot(normal,lightdir),0)*lightcolorDiffuse)*diffuseColor; 
   return ret;
}


float4 ps_main(float4 ScreenPos        : POSITION,
               float2 texcoord        : TEXCOORD0,
               float3 ViewSpaceNormal : TEXCOORD1,
               float3 ViewSpacePos     : TEXCOORD2, 
               float4 ViewSpaceLight0Dir :TEXCOORD3,
               float4 ViewSpaceLight1Dir :TEXCOORD4,
               float4 ViewSpaceLight2Dir :TEXCOORD5,
               float4 ViewSpaceLight3Dir :TEXCOORD6) : COLOR0
{   
   float texelColor=tex2D(DiffuseTexture,texcoord);
   float fragmentColor=lightAmbientColor*texelColor;
   ViewSpaceNormal=normalize(ViewSpaceNormal);
   
   ViewSpacePos = normalize(ViewSpacePos*-1.0f);   
   ViewSpaceLight0Dir.xyz = normalize(ViewSpaceLight0Dir.xyz);
   ViewSpaceLight1Dir.xyz = normalize(ViewSpaceLight1Dir.xyz);
   ViewSpaceLight2Dir.xyz = normalize(ViewSpaceLight2Dir.xyz);
   ViewSpaceLight3Dir.xyz = normalize(ViewSpaceLight3Dir.xyz);
   fragmentColor+=diffuse_specular_phong(ViewSpaceLight0Dir.xyz,ViewSpaceNormal,ViewSpacePos,texelColor,lightSpecularColor0,lightDiffuseColor0);
   fragmentColor+=diffuse_specular_phong(ViewSpaceLight1Dir.xyz,ViewSpaceNormal,ViewSpacePos,texelColor,lightSpecularColor1,lightDiffuseColor1);
   fragmentColor+=diffuse_specular_phong(ViewSpaceLight2Dir.xyz,ViewSpaceNormal,ViewSpacePos,texelColor,lightSpecularColor2,lightDiffuseColor2);
   //fragmentColor+=diffuse_specular_phong(ViewSpaceLight3Dir.xyz,ViewSpaceNormal,ViewSpacePos,texelColor,light3SpecularColor,light3DiffuseColor);
   return fragmentColor;
   
}




]]></RmShaderCode>
