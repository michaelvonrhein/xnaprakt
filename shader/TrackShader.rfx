     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float3 lightDirection0;
float3 lightDirection1;
float3 lightDirection2;
float3 lightDirection3;

float3 lightPosition0;
float3 lightPosition1;
float3 lightPosition2;
float3 lightPosition3;

float maxLightRangeSQ;

float4x4 view;
float4x4 worldView;
float4x4 worldViewIT;
float4x4 worldViewProjection;

struct VS_INPUT
{
   float4 Position   : POSITION;
   float3 Normal   : NORMAL;
   float2 texcoord   : TEXCOORD0;
   float3 tangent  : TANGENT;
};



struct VS_OUTPUT 
{
   float4 ScreenPos        : POSITION;
   float2 TexCoord        : TEXCOORD0;
   float3 TexSpaceViewDir   : TEXCOORD1; 
   float4 TexSpaceLight0Dir :TEXCOORD2;
   float4 TexSpaceLight1Dir :TEXCOORD3;
   float4 TexSpaceLight2Dir :TEXCOORD4;
   float4 TexSpaceLight3Dir :TEXCOORD5;
};





VS_OUTPUT vs_main( float4 Position   : POSITION,
   float3 Normal   : NORMAL,
   float2 TexCoord   : TEXCOORD0,
   float3 Tangent  : TANGENT )
{
   VS_OUTPUT Out;
   float3x3 texSpaceMatrix;
   Out.ScreenPos = mul(float4(Position.xyz,1), worldViewProjection);
   Out.TexSpaceViewDir=mul(Position, worldView);
   Out.TexSpaceLight0Dir=mul(float4(lightPosition0.xyz,1),view)-float4(Out.TexSpaceViewDir,0);
   Out.TexSpaceLight1Dir=mul(float4(lightPosition1.xyz,1),view)-float4(Out.TexSpaceViewDir,0);
   Out.TexSpaceLight2Dir=mul(float4(lightPosition2.xyz,1),view)-float4(Out.TexSpaceViewDir,0);
   Out.TexSpaceLight3Dir=mul(float4(lightPosition3.xyz,1),view)-float4(Out.TexSpaceViewDir,0);
   Out.TexSpaceLight0Dir.w=max((maxLightRangeSQ-dot(Out.TexSpaceLight0Dir,Out.TexSpaceLight0Dir))/maxLightRangeSQ,0);
   Out.TexSpaceLight1Dir.w=max((maxLightRangeSQ-dot(Out.TexSpaceLight1Dir,Out.TexSpaceLight1Dir))/maxLightRangeSQ,0);
   Out.TexSpaceLight2Dir.w=max((maxLightRangeSQ-dot(Out.TexSpaceLight2Dir,Out.TexSpaceLight2Dir))/maxLightRangeSQ,0);
   Out.TexSpaceLight3Dir.w=max((maxLightRangeSQ-dot(Out.TexSpaceLight3Dir,Out.TexSpaceLight3Dir))/maxLightRangeSQ,0);
   Out.TexCoord=TexCoord;
   
   //build tex space matrix
   texSpaceMatrix[0]=mul(float4(Tangent.xyz,0),worldViewIT).xyz;
   texSpaceMatrix[2]=mul(float4(Normal.xyz,0),worldViewIT).xyz;
   texSpaceMatrix[1]=float4(cross(texSpaceMatrix[0],texSpaceMatrix[2]),0).xyz;
    
   //transform Lightpos and viewpos into texspace
   Out.TexSpaceViewDir=mul(texSpaceMatrix,Out.TexSpaceViewDir*-1.0f);
   Out.TexSpaceLight0Dir.xyz=mul(texSpaceMatrix,Out.TexSpaceLight0Dir.xyz);
   Out.TexSpaceLight1Dir.xyz=mul(texSpaceMatrix,Out.TexSpaceLight1Dir.xyz);
   Out.TexSpaceLight2Dir.xyz=mul(texSpaceMatrix,Out.TexSpaceLight2Dir.xyz);
   Out.TexSpaceLight3Dir.xyz=mul(texSpaceMatrix,Out.TexSpaceLight3Dir.xyz);

   
   
   return Out;  
}



]]></RmShaderCode>
