     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 materialAmbientColor;
float4 materialSpecularColor;
float  materialGlossiness;

float4 lightAmbientColor;

float4 lightDiffuseColor0;
float4 lightDiffuseColor1;
float4 lightDiffuseColor2;
float4 lightDiffuseColor3;

float4 lightSpecularColor0;
float4 lightSpecularColor1;
float4 lightSpecularColor2;
float4 lightSpecularColor3;

sampler2D DiffuseTexture;

sampler2D NormalMap;

sampler2D ShadowMap0;
sampler2D ShadowMap1;
sampler2D ShadowMap2;
sampler2D ShadowMap3;

struct PS_INPUT 
{
   float4 ScreenPos          : POSITION;
   float3 ViewSpacePos       : TEXCOORD0;
   float2 TexCoord           : TEXCOORD1;
   float4 ViewSpaceLightDir0 : TEXCOORD2;
   float4 ViewSpaceLightDir1 : TEXCOORD3;
   float4 ViewSpaceLightDir2 : TEXCOORD4;
   float4 ViewSpaceLightDir3 : TEXCOORD5;
   float4 ShadowMapTexCoord0 : TEXCOORD6;
   float4 ShadowMapTexCoord1 : TEXCOORD7;
   float4 ShadowMapTexCoord2 : TEXCOORD8;
   float4 ShadowMapTexCoord3 : TEXCOORD9;
};

inline float3 reflectionVector(float3 v,float3 reflectAbout)
{
   float3 ret;
   ret=(2.0f*(dot(reflectAbout,v)))*reflectAbout-v;
   return ret;
}

//compute ligthing with the blinn_phong model for specular lighting
inline float4 diffuse_specular_blinn(float3 lightdir, float3 normal, float3 viewdir,
                             float4 diffuseColor,
                             float4 lightcolorSpecular,float4 lightcolorDiffuse)
{
   float4 specpart=0,diffpart=0;
   float3 h,vl;
   vl=viewdir+lightdir;
   h=vl/length(vl);
   specpart=(pow(max(dot(normal,h),0),materialGlossiness)*lightcolorSpecular) * materialSpecularColor;
   diffpart=(max(dot(normal,lightdir),0)*lightcolorDiffuse)*diffuseColor; 
   return specpart+diffpart;
}

inline bool checkRange(float x, float y)
{
   return saturate(x) == x && saturate(y) == y;
}

inline float4 alignTexCoord(float4 input)
{
   input.x = input.x / input.w / 2.0f + 0.5f;
   input.y = input.y / input.w / -2.0f + 0.5f;
   return input;
}

float4 ps_main(PS_INPUT In) : COLOR0
{   
   float4 texelColor      = tex2D(DiffuseTexture, In.TexCoord);
   float3 viewSpaceNormal = 2.0f * (tex2D(NormalMap,In.TexCoord).xyz - 0.5f);
   float  fragmentColor   = lightAmbientColor * texelColor;
   
   In.ViewSpaceLightDir0.xyz = normalize(In.ViewSpaceLightDir0.xyz);
   In.ViewSpacePos = normalize(In.ViewSpacePos);
   
   // Realign texture coordinates from (-1..1) to (0..1)
   In.ShadowMapTexCoord0 = alignTexCoord(In.ShadowMapTexCoord0);
   In.ShadowMapTexCoord1 = alignTexCoord(In.ShadowMapTexCoord1);
   In.ShadowMapTexCoord2 = alignTexCoord(In.ShadowMapTexCoord2);
   In.ShadowMapTexCoord3 = alignTexCoord(In.ShadowMapTexCoord3);
   
   // Check if pixel is within light range (0..1)
   if(checkRange(In.ShadowMapTexCoord0.x, In.ShadowMapTexCoord0))
   {
      // w component encodes depth
      if((In.ViewSpaceLightDir0.w - 0.01f) <= tex2D(ShadowMap0, In.ShadowMapTexCoord0).x)
      {
         fragmentColor += diffuse_specular_blinn(In.ViewSpaceLightDir0.xyz, viewSpaceNormal, In.ViewSpacePos, texelColor, lightSpecularColor0, lightDiffuseColor0);
      }
   }
   
   // Check if pixel is within light range (0..1)
   if(checkRange(In.ShadowMapTexCoord1.x, In.ShadowMapTexCoord1))
   {
      // w component encodes depth
      if((In.ViewSpaceLightDir1.w - 0.01f) <= tex2D(ShadowMap1, In.ShadowMapTexCoord1).x)
      {
         fragmentColor += diffuse_specular_blinn(In.ViewSpaceLightDir1.xyz, viewSpaceNormal, In.ViewSpacePos, texelColor, lightSpecularColor1, lightDiffuseColor1);
      }
   }
   
   // Check if pixel is within light range (0..1)
   if(checkRange(In.ShadowMapTexCoord2.x, In.ShadowMapTexCoord2))
   {
      // w component encodes depth
      if((In.ViewSpaceLightDir2.w - 0.01f) <= tex2D(ShadowMap2, In.ShadowMapTexCoord2).x)
      {
         fragmentColor += diffuse_specular_blinn(In.ViewSpaceLightDir2.xyz, viewSpaceNormal, In.ViewSpacePos, texelColor, lightSpecularColor2, lightDiffuseColor2);
      }
   }
   
   // Check if pixel is within light range (0..1)
   if(checkRange(In.ShadowMapTexCoord3.x, In.ShadowMapTexCoord3))
   {
      // w component encodes depth
      if((In.ViewSpaceLightDir3.w - 0.01f) <= tex2D(ShadowMap3, In.ShadowMapTexCoord3).x)
      {
         fragmentColor += diffuse_specular_blinn(In.ViewSpaceLightDir3.xyz, viewSpaceNormal, In.ViewSpacePos, texelColor, lightSpecularColor3, lightDiffuseColor3);
      }
   }
   
   return saturate(fragmentColor);
}


]]></RmShaderCode>
