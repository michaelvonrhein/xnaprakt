sampler2D TDDiffuse1;


float4 materialAmbientColor;

float4 lightAmbientColor;

float4 lightDiffuseColor0;
float4 lightDiffuseColor1;

float4 lightSpecularColor0;
float4 lightSpecularColor1;




inline float3 reflectionVector(float3 v,float3 reflectAbout)
{
   float3 ret;
   ret=(2.0f*(dot(reflectAbout,v)))*reflectAbout-v;
   return ret;
}



//compute ligthing with the phong model for specular lighting
float4 diffuse_specular_phong(float3 lightdir, float3 normal, float3 viewdir,
                             float4 diffuseColor,
                             float lightcolorSpecular,float lightcolorDiffuse,
                             float materialGlossiness,
                             float4 materialSpecularColor)
{

   float4 specpart=0,diffpart=0;
   float3 reflected=reflectionVector(lightdir,normal);
   specpart=(pow(max(dot(viewdir,reflected),0),materialGlossiness)*lightcolorSpecular) * materialSpecularColor;
    diffpart=(max(dot(normal,lightdir),0)*lightcolorDiffuse)*diffuseColor;
    return specpart+diffpart;
}

//compute ligthing with the blinn_phong model for specular lighting
float4 diffuse_specular_blinn(float3 lightdir, float3 normal, float3 viewdir,
                             float4 diffuseColor,
                             float lightcolorSpecular,
                             float lightcolorDiffuse,
                             float materialGlossiness,
                             float4 materialSpecularColor)
{
   float4 specpart=0,diffpart=0;
   float3 h,vl;
   vl=viewdir+lightdir;
   h=vl/length(vl);
   specpart=(pow(max(dot(normal,h),0),materialGlossiness)*lightcolorSpecular) * materialSpecularColor;
   diffpart=(max(dot(normal,lightdir),0)*lightcolorDiffuse)*diffuseColor; 
    return specpart+diffpart;
}

inline float4 blend(float4 color1, float4 color2,float alpha)
{
   return color1*(1.0f-color2.w*alpha)+color2*color2.w*alpha;
}


float4 ps_main(float4 ScreenPos        : POSITION,
               float2 texcoord0        : TEXCOORD0,
               float2 texcoord1        : TEXCOORD1,
               float3 ViewSpacePos     : TEXCOORD2, 
               float4 ViewSpaceLight0Dir :TEXCOORD3,
               float4 ViewSpaceLight1Dir :TEXCOORD4) : COLOR0
{   
   float4 ViewSpaceNormal;
   float4 texelColor=0;
   float4 tdv;
   float2 texcoord2=float2(texcoord1.x%1,texcoord1.y%1)*0.5f;
   float4 specParam;
   tdv=tex2D(TDDiffuse1,texcoord0);
   texelColor=blend(texelColor,tex2D(DiffuseTexture1,texcoord2),tdv.x);
   texelColor=blend(texelColor,tex2D(DiffuseTexture2,texcoord2),tdv.y);
   texelColor=blend(texelColor,tex2D(DiffuseTexture3,texcoord2),tdv.z);
   texelColor=blend(texelColor,tex2D(DiffuseTexture4,texcoord2),tdv.x);
   tdv=tex2D(TDDiffuse2,texcoord0);
   //texelColor=blend(texelColor,tex2D(DiffuseTexture5,texcoord2),tdv.x);
   //texelColor=blend(texelColor,tex2D(DiffuseTexture6,texcoord2),tdv.y);
   specParam=tex2D(SpecularMaps,texcoord2+tdv.zw);
   tdv=tex2D(TDNormalAlpha,texcoord0);
   ViewSpaceNormal=2.0f*(tex2D(NormalMaps,texcoord2+tdv.xy)-0.5f);
   float4 fragmentColor=lightAmbientColor*texelColor;
   ViewSpacePos = normalize(ViewSpacePos);   
   ViewSpaceLight0Dir.xyz = normalize(ViewSpaceLight0Dir.xyz);
   //ViewSpaceLight1Dir.xyz = normalize(ViewSpaceLight1Dir.xyz);
   
   
   //float4 specParam= float4(1,1,1,1);
   fragmentColor+=diffuse_specular_blinn(ViewSpaceLight0Dir.xyz,ViewSpaceNormal,ViewSpacePos,texelColor,lightSpecularColor0,lightDiffuseColor0,specParam.w*255.0f,float4(specParam.xyz,1));
   //fragmentColor+=diffuse_specular_blinn(ViewSpaceLight1Dir.xyz,ViewSpaceNormal,ViewSpacePos,texelColor,lightSpecularColor1,lightDiffuseColor1,specParam.w*255.0f,float4(specParam.xyz,1));
   
   fragmentColor.w=tex2D(AlphaMaps,texcoord2+tdv.zw).x;
   fragmentColor.w=1;
   return fragmentColor;
   
}




